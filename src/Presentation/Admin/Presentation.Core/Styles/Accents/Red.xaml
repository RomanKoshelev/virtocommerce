<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    
    <Color x:Key="ShellBackgroundLightColor">#FFE9E9E9</Color>
    <Color x:Key="ShellHighlightLightColor">#FFCFCFCF</Color>
    <Color x:Key="ShellForegroundLightColor">#FF626262</Color>
    <Color x:Key="ShellMenuDarkColor">#FF0F0F0F</Color>
    <Color x:Key="ShellBackgroundDarkColor">#FF3B3B3B</Color>
    <Color x:Key="ShellHighlightDarkColor">#FF525252</Color>

    <Color x:Key="HighlightDarkColor">#FFcb0000</Color>
    <Color x:Key="HighlightLightColor">#FFcb0000</Color>

    <Color x:Key="AccentColor">#CCcb0000</Color>
    <Color x:Key="AccentDarkColor">#FF1c739d</Color>

    <!--80%-->
    <Color x:Key="AccentColor1">#596063</Color>
    <!--60%-->
    <Color x:Key="AccentColor2">#ABA1A0</Color>
    <!--40%-->
    <Color x:Key="AccentColor3">#DBD0D0</Color>
    <!--20%-->
    <Color x:Key="AccentColor4">#33119EDA</Color>

    <LinearGradientBrush x:Key="BrandingBrush"
                         EndPoint="0.001,0.5"
                         StartPoint="1.002,0.5">
        <GradientStop Color="{DynamicResource HighlightDarkColor}"
                      Offset="0" />
        <GradientStop Color="#FF5FC316"
                      Offset="1" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="AccentColorBrush"
                     Color="{DynamicResource AccentColor}" />




</ResourceDictionary>