<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
					xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
					mc:Ignorable="PresentationOptions"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!--Window-->
	<SolidColorBrush x:Key="Virto_Window_Background_Brush" Color="#2D2D30" PresentationOptions:Freeze="true" />

	<SolidColorBrush x:Key="Virto_Window_Foreground_Brush" Color="#B3B3B3" PresentationOptions:Freeze="true" />

	<!--Window--> 

	<!--2. TextBlock brushes-->
	<!--2.0 -->
	<SolidColorBrush x:Key="Virto_TextBlock_Foreground_Brush" Color="#B19D7D" PresentationOptions:Freeze="true"/>

	<SolidColorBrush x:Key="Virto_TextBlock_Foreground_Title_Brush" Color="#FFFFFF" PresentationOptions:Freeze="true" />

	<SolidColorBrush x:Key="Virto_TextBlock_Foreground_SubTitle_Brush" Color="#B19D7D" PresentationOptions:Freeze="true"/>

	<!--TextBlock brushes-->

	<!--3. TextBox brashes -->
	<!--3.1 -->
	<SolidColorBrush x:Key="Virto_TextBox_Foreground_Brush" Color="#B3B3B3" PresentationOptions:Freeze="true" />
	<!--3.2 -->
	<SolidColorBrush x:Key="Virto_TextBox_Background_Brush" Color="#343446" PresentationOptions:Freeze="true"/>
	<!--3.3 -->
	<SolidColorBrush x:Key="Virto_TextBox_Border_Brush" Color="#777777" PresentationOptions:Freeze="true"/>
	<!--3.4 -->
	<SolidColorBrush x:Key="Virto_TextBox_BorderDisable_Brush" Color="Gray" PresentationOptions:Freeze="true"/>
	<!--3.5 -->
	<SolidColorBrush x:Key="Virto_TextBox_BorderMouseOver_Brush" Color="#51A3FD" PresentationOptions:Freeze="true"/>
	<!--3.6-->
	<SolidColorBrush x:Key="Virto_TextBox_Background_MouseOver_Brush" Color="#484858" PresentationOptions:Freeze="true"/>
	<!--3.7-->
	<SolidColorBrush x:Key="Virto_TextBox_Foreground_Selected_Brush" Color="White" PresentationOptions:Freeze="true"/>
	<!-- TextBox brashes -->


	<!--6. Buttons brushes-->
	<!--6.1 -->
	<SolidColorBrush x:Key="Virto_Button_Background_Normal_Brush" Color="#252526" PresentationOptions:Freeze="true"/>
	<!--6.2 -->
	<SolidColorBrush x:Key="Virto_Button_Foreground_Normal_Brush" Color="#B3B3B3" PresentationOptions:Freeze="true"/>
	<!--6.3 -->
	<SolidColorBrush x:Key="Virto_Button_BorderBrush_Normal_Brush" Color="#626263" PresentationOptions:Freeze="true" />
	<!--6.4-->
	<SolidColorBrush x:Key="Virto_Button_Background_MouseOver_Brush" Color="#3A3A3B" PresentationOptions:Freeze="true"/>
	<!--6.5-->
	<SolidColorBrush x:Key="Virto_Button_Background_Pressed_Brush" Color="#343435" PresentationOptions:Freeze="true"/>

	<SolidColorBrush x:Key="Virto_Button_BorderBrush_MouseOver_Brush" Color="#51A3FD" PresentationOptions:Freeze="true"/>
	<SolidColorBrush x:Key="Virto_Button_BorderBrush_Pressed_Brush" Color="#53A3FD" PresentationOptions:Freeze="true"/>

	<!--Buttons brushes-->

	<!--7. ProgressBar brushes-->
	<!--7.2 -->
	<SolidColorBrush x:Key="Virto_ProgressBar_Foreground_Brush" Color="#51A3FD" PresentationOptions:Freeze="true"/>
	<!--ProgressBar brushes-->

	<!--9. CheckBox brushes-->
	<!--9.1 -->
	<SolidColorBrush x:Key="Virto_CheckBox_Border_Brush" Color="#777777" PresentationOptions:Freeze="true"/>
	<!--9.2 -->
	<SolidColorBrush x:Key="Virto_CheckBox_Background_Brush" Color="#373748" PresentationOptions:Freeze="true"/>
	<!--9.3 -->
	<SolidColorBrush x:Key="Virto_CheckBox_Content_Default_Foreground_Brush" Color="#B3B3B3" PresentationOptions:Freeze="true"/>
	<!--9.4 -->
	<SolidColorBrush x:Key="Virto_CheckBox_Content_MouseOver_Foreground_Brush" Color="White" PresentationOptions:Freeze="true"/>
	<!--9.5 -->
	<SolidColorBrush x:Key="Virto_CheckBox_Foreground_Brush" Color="#B3B3B3" PresentationOptions:Freeze="true"/>

	<SolidColorBrush x:Key="Virto_CheckBox_BorderBrush_MouseOver_Brush" Color="#51A3FD" PresentationOptions:Freeze="true"/>
	<SolidColorBrush x:Key="Virto_CheckBox_Background_MouseOver_Brush" Color="#484858" PresentationOptions:Freeze="true"/>
	<!--CheckBox brushes-->

	<!--10. RadioButton brushes-->
	<!--10.1 -->
	<SolidColorBrush x:Key="Virto_RadioButton_Ellipse_Checked_Stroke" Color="#777777" PresentationOptions:Freeze="true"/>
	<!--10.2 -->
	<SolidColorBrush x:Key="Virto_RadioButton_Ellipse_MouseOver_Stroke" Color="#3296D4" PresentationOptions:Freeze="true"/>
	<!--10.3 -->
	<SolidColorBrush x:Key="Virto_RadioButton_Content_Default_Foreground" Color="#B3B3B3" PresentationOptions:Freeze="true"/>
	<!--10.4 -->
	<SolidColorBrush x:Key="Virto_RadioButton_Content_MouseOver_Foreground" Color="White" PresentationOptions:Freeze="true"/>
	<!--10.5-->
	<SolidColorBrush x:Key="Virto_RadioButton_Border_Fill_Brush" Color="#343446" PresentationOptions:Freeze="true"/>
	<!--RadioButton brushes-->

	<!--21. HyperLink-->
	<!--21.01-->
	<SolidColorBrush x:Key="Virto_HyperLink_Foreground_Brush" Color="#3A94E4" PresentationOptions:Freeze="true"/>

	<SolidColorBrush x:Key="Virto_HyperLink_MouseOver_Foreground_Brush" Color="#64A9FE" PresentationOptions:Freeze="true"/>

</ResourceDictionary>