<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:core_controls="clr-namespace:VirtoCommerce.ManagementClient.Core.Controls;assembly=VirtoCommerce.ManagementClient.Core"                    
                    xmlns:localView="clr-namespace:VirtoCommerce.ManagementClient.Configuration.View"
                    xmlns:implementations="clr-namespace:VirtoCommerce.ManagementClient.Configuration.ViewModel.Implementations">

    

    <DataTemplate DataType="{x:Type implementations:ConfigurationHomeViewModel}">
        <core_controls:CacheContentControl Type="{x:Type localView:ConfigurationHomeView}"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type implementations:BuildSettingsViewModel}">
        <core_controls:CacheContentControl Type="{x:Type localView:BuildSettingsView}"/>
    </DataTemplate>
    
</ResourceDictionary>